scalar Time

type Odd {
    id: String!
    sportKey: String!
    sportTitle: String!
    commenceTime: Time!
    homeTeam: String!
    awayTeam: String!
    bookmakers: [Bookmaker]
}

type Bookmaker {
    key: String!
    title: String!
    lastUpdate: Time!
    markets: [Market]
}

type Market {
    key: String!
    lastUpdate: Time!
    outcomes: [Outcome]
}

type Outcome {
    name: String!
    price: String!
}

type Query {
    odds: [Odd!]!
}